{
  "buildCommand": "ng build",
  "outputDirectory": "dist/personality-test-app/browser",
  "devCommand": "ng serve --port 4200",
  "framework": "angular",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "CLAUDE_API_KEY": "@claude-api-key"
  }
}