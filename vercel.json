{
  "version": 2,
  "buildCommand": "ng build --configuration production",
  "outputDirectory": "dist/marvick-personality-assessment",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}